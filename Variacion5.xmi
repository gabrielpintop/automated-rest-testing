<?xml version="1.0" encoding="UTF-8"?>
<AutomatedRestTesting:AutomatedRestTesting
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:AutomatedRestTesting="http://www.example.org/AutomatedRestTesting">
  <elements
      xsi:type="AutomatedRestTesting:DomainEntity"
      name="Student">
    <domainAttributes
        name="first_name"/>
    <domainAttributes
        name="last_name"/>
    <domainAttributes
        name="gender"/>
    <domainAttributes
        name="code"
