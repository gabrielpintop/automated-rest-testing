        name="GetBody">
      <requestAttributes
          name="AnyAttribute"/>
    </requestBody>
  </elements>
  <elements
      xsi:type="AutomatedRestTesting:RestService"
      name="GetStatesByCountry"
      uri="/countries/{country}/states"
      resources="//@elements.1">
    <requestBody
        name="GetBody">
      <requestAttributes
          name="AnyAttribute"/>
    </requestBody>
  </elements>
  <elements
      xsi:type="AutomatedRestTesting:RestService"
      name="GetCitiesByCountry"
      uri="/countries/{country}/cities"
      resources="//@elements.1">
    <requestBody
        name="GetBody">
      <requestAttributes
          name="AnyAttribute"/>
    </requestBody>
  </elements>
  <elements
      xsi:type="AutomatedRestTesting:RestService"
      name="GetStudents"
      uri="/students"
      resources="//@elements.0">
    <requestBody
        name="GetBody">
      <requestAttributes
          name="AnyAttribute"/>
    </requestBody>
  </elements>
  <elements
      xsi:type="AutomatedRestTesting:RestService"
      name="AddStudent"
      uri="/students"
      resources="//@elements.0"/>
  <elements
      xsi:type="AutomatedRestTesting:RestService"
      name="GetStudentById"
      uri="/students/{id}"
      resources="//@elements.0">
    <requestBody
        name="GetBody">
      <requestAttributes
          name="AnyAttribute"/>
    </requestBody>
  </elements>
  <elements
      xsi:type="AutomatedRestTesting:RestService"
      name="DeleteStudent"
      uri="/students/{id}"
      resources="//@elements.0"
      method="Delete"/>
</AutomatedRestTesting:AutomatedRestTesting>
