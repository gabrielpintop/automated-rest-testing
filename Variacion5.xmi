      name="Country">
    <domainAttributes
        name="shortName"
        primaryKey="true"/>
    <domainAttributes
        name="name"/>
    <domainAttributes
        name="native"/>
    <domainAttributes
        name="phone"/>
    <domainAttributes
        name="continent"/>
    <domainAttributes
        name="capital"/>
    <domainAttributes
        name="currency"/>
